package layout

templ Masthead(slogan string) {
    <header class="masthead">
        <div class="container">
            <div class="masthead-subheading">Welcome to CompliCode!</div>
            <div class="masthead-heading text-uppercase">{ slogan }</div>
            <a class="btn btn-primary btn-xl text-uppercase" href="#services">Tell Me More</a>
        </div>
    </header>
}